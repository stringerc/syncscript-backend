# Production Environment Variables for SyncScript Backend
NODE_ENV=production
PORT=3001
FRONTEND_URL=https://syncscript.app

# Database (will be configured when you set up production database)
DATABASE_URL=postgresql://username:password@host:5432/syncscript
REDIS_URL=redis://username:password@host:6379

# Authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
AUTH0_DOMAIN=your-auth0-domain.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret

# AI Services
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
PINECONE_API_KEY=your-pinecone-api-key

# SendGrid (will be configured after domain setup)
SENDGRID_API_KEY=your-sendgrid-api-key

# AWS (for file storage)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_S3_BUCKET=your-s3-bucket-name

# Search
ELASTICSEARCH_URL=http://localhost:9200
